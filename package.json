{
  "name": "coc-inline-jest",
  "version": "0.1.2",
  "description": "Like VSCode-Jest, but for vim",
  "main": "lib/Core/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "webpack --silent || exit 0",
    "prepare": "npx npm-run-all clean build",
    "dev": "webpack --watch"
  },
  "engines": {
    "coc": ">=0.0.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khanghoang/coc-jest.git"
  },
  "keywords": [
    "coc.nvim",
    "jest"
  ],
  "author": "hoangtrieukhang@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khanghoang/coc-jest/issues"
  },
  "homepage": "https://github.com/khanghoang/coc-jest#readme",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:javascript.jsx",
    "onLanguage:typescript",
    "onLanguage:typescript.tsx",
    "onLanguage:typescript.jsx",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jest",
      "properties": {
        "jest.terminalPosition": {
          "type": "string",
          "description": "Position of terminal window.",
          "default": "right",
          "enum": [
            "bottom",
            "right"
          ]
        },
        "jest.enabled": {
          "type": "boolean",
          "default": true
        },
        "jest.pathToConfig": {
          "type": "string",
          "default": "./jest.config.js"
        },
        "jest.pathToJest": {
          "type": "string",
          "default": "./node_modules/.bin/jest"
        },
        "jest.pathToRoot": {
          "type": "string",
          "default": "./"
        }
      }
    },
    "commands": [
      {
        "title": "Toggle Jest Inline",
        "category": "Jest",
        "command": "inlineJest.toggle"
      }
    ]
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.17",
    "@types/node": "^10.12.9",
    "@types/which": "^1.3.1",
    "coc.nvim": "^0.0.73",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.0.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "jest": "^24.9.0",
    "jest-editor-support": "26.0.0-beta",
    "js-yaml": "^3.13.1",
    "mixin-deep": "^2.0.1",
    "set-value": "^4.0.1",
    "vscode-languageserver-protocol": "^3.14.1",
    "which": "^1.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
